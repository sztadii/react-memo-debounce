{
  "name": "react-memo-debounce",
  "version": "1.1.0",
  "description": "A function to prevent unnecessary render calls ( almost like React.memo )",
  "tags": [
    "react",
    "memo",
    "debounce"
  ],
  "keywords": [
    "react",
    "memo",
    "debounce"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublish": "npm run build && npm run build-types",
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc -p tsconfig-build.json",
    "build-types": "tsc -p tsconfig-types.json",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "type-check-watch": "tsc --noEmit --watch",
    "test": "jest --silent",
    "test-watch": "jest --watchAll",
    "lint": "esw \"./src/**/*.{ts,tsx}\" --ext=\".tsx,.ts\" --ignore-path .gitignore --color",
    "lint-watch": "npm run lint -- --watch --cache --clear",
    "lint-names": "file-naming-enforcer type=camelCase folder=./src",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{ts,tsx,js,json,scss,css}\""
  },
  "dependencies": {
    "lodash": "^4.17.20",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "peerDependencies": {
    "lodash": "^4.17.20",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@testing-library/react": "^10.4.9",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.161",
    "@types/react": "^16.9.49",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-jest": "^26.3.0",
    "eslint": "^7.8.1",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-react": "^7.20.6",
    "eslint-watch": "^6.0.1",
    "file-naming-enforcer": "^1.1.8",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "lint-staged": "^10.3.0",
    "prettier": "^2.1.1",
    "typescript": "^3.9.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,json}": [
      "prettier --write --"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sztadii/react-memo-debounce.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sztadii/react-memo-debounce/issues"
  },
  "homepage": "https://github.com/sztadii/react-memo-debounce#readme"
}
